<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Home</servlet-name>
  	<display-name>Home</display-name>
  	<description></description>
  	<servlet-class>controller.Home</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserServelet</servlet-name>
  	<display-name>UserServelet</display-name>
  	<description></description>
  	<servlet-class>controller.UserServelet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Home</servlet-name>
  	<url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserServelet</servlet-name>
  	<url-pattern>/UserServelet</url-pattern>
  </servlet-mapping>
</web-app>
